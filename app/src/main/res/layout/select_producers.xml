<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/AbsoluteLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/WHITE" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="458dp" >

        <AbsoluteLayout
            android:id="@+id/AbsoluteLayout1"
            android:layout_width="match_parent"
            android:layout_height="465dp" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#000"
                android:text="                           SELECIONE OS PRODUTORES PARA DOWNLOAD DO PAPER TOP                                                       CLIQUE SOBRE O PRODUTOR PARA REMOVE-LO DA LISTA"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/WHITE"
                android:textSize="13dp"
                android:textStyle="bold" />

            <ListView
                android:id="@+id/lstResult"
                android:layout_width="492dp"
                android:layout_height="427dp"
                android:layout_x="513dp"
                android:layout_y="19dp"
                android:background="#fff2e5"
                android:choiceMode="multipleChoice" >
            </ListView>

            <ListView
                android:id="@+id/lstProducers"
                android:layout_width="492dp"
                android:layout_height="430dp"
                android:layout_x="4dp"
                android:layout_y="19dp"
                android:choiceMode="multipleChoice" >
            </ListView>

        </AbsoluteLayout>
    </ScrollView>

    <Button
        android:id="@+id/btnDow"
        android:layout_width="454dp"
        android:layout_height="wrap_content"
        android:layout_x="-2dp"
        android:layout_y="473dp"
        android:elevation="1dp"
        android:text="DOWNLOAD PAPER TOP" />

    <Button
        android:id="@+id/btnAlternativo"
        android:layout_width="442dp"
        android:layout_height="wrap_content"
        android:layout_x="541dp"
        android:layout_y="472dp"
        android:text="DOWNLOAD PAPER TOP ALTERNATIVO" />

</AbsoluteLayout>
